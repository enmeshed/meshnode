// The ExtensionHeader is a resource passed between nodes of the mesh with
// each RPC and HTTP call. It identifies the user requesting the call and
// carries security and other data between nodes.
import { Container, toProvider } from '@enmeshed/alpha'
import { ExtensionHeader } from '../util/ExtensionHeader'

Container.provide(toProvider({
  name: "extensionHeader"

  injectDependencies(deps, setupContext) -/>
    ExtensionHeader

  init(rsrc) -/>
    Container.retain(rsrc)
}))
